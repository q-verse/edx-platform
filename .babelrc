{
  "plugins": [
    "transform-object-assign",
    "transform-object-rest-spread",
    "transform-class-properties"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "IE >= 11"
          ]
        },
        "modules": false
      }
    ],
    "babel-preset-react"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs",
          }
        ]
      ]
    }
  },
}
